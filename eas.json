{
  "cli": {
    "version": ">= 14.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "development-apk": {
      "android": {
        "buildType": "apk",
        "releaseChannel": "production"
      },
      "distribution": "internal",
      "developmentClient": false,
      "env": {
        "EXPO_NO_DEV": "1",
        "NODE_ENV": "production",
        "APP_ENV": "production"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
