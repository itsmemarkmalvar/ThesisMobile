{
  "expo": {
    "name": "BiniBaby",
    "slug": "binibaby",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Binibaby (2).png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Binibaby (2).png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.binibaby.app",
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos",
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos"
      },
      "associatedDomains": [
        "applinks:binibaby-api.com"
      ]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Binibaby (2).png",
        "backgroundColor": "#FFFFFF"
      },
      "icon": "./assets/Binibaby (2).png",
      "package": "com.binibaby.app",
      "permissions": [
        "INTERNET",
        "android.permission.INTERNET"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "binibaby",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [],
    "owner": "binibabymobileapp",
    "scheme": "binibaby",
    "extra": {
      "eas": {
        "projectId": "70548d8d-b2c6-45a5-83f5-4e58d4d5c80a"
      }
    }
  }
}
